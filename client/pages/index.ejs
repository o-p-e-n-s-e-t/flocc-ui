<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="https://unpkg.com/flocc"></script>
    <script src="/dist/flocc-ui.js"></script>
  </head>
  <body>
    <div id="value">0</div>
    <div id="container"></div>
    <script>
      const { Slider } = floccUI;
      new Slider({
        min: -5,
        max: 10,
        step: 0.5,
        value: 0,
        style: { border: "2px solid red", marginBottom: 20, width: 300 },
      }).mount("#container");
      new Slider({
        min: -10,
        max: 10,
        step: 0.5,
        value: 0,
        style: { width: 300 },
      }).mount("#container");
    </script>
  </body>
</html>
